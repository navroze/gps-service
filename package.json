{
    "name": "gps-test",
    "version": "1.0.0",
    "description": "GPS GRPC microservice in typescript",
    "main": "index.js",
    "scripts": {
        "test": "NODE_ENV=development mocha --diff --check-leaks -r ts-node/register -r tsconfig-paths/register \"src/test/**/*.spec.ts\"",
        "proto:gen":"./src/proto-gen.sh",
        "dev": "NODE_ENV=development nodemon -r tsconfig-paths/register src/index.ts",
        "prod": "NODE_ENV=production pm2 start build/server.js --watch -i max"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navroze/gps-service.git"
    },
    "keywords": [
        "GPS",
        "GRPC"
    ],
    "engines": {
        "npm": ">=7.10.0",
        "node": ">=16.0.0"
    },
    "author": "Navroze Bomanji",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/navroze/gps-service/issues"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.8.7",
        "dotenv": "^16.0.3",
        "moment": "^2.29.4",
        "mongoose": "^6.8.4",
        "pm2": "^5.2.2",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/dotenv": "^8.2.0",
        "@types/mocha": "^10.0.1",
        "@types/moment": "^2.13.0",
        "@types/mongoose": "^5.11.97",
        "@types/sinon": "^10.0.13",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "chai": "^4.3.7",
        "eslint": "^8.33.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.20",
        "nyc": "^15.1.0",
        "sinon": "^15.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5"
    },
    "homepage": "https://github.com/navroze/gps-service#readme"
}
